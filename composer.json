{
    "name": "pnoexz/slim4-skeleton",
    "description": "Skeleton for Slim 4",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.2",
        "pnoexz/json": "^1.1",
        "psr/log": "^1.1",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matias Pino",
            "email": "pnoexz@gmail.com"
        }
    ],
    "require-dev": {
        "slevomat/coding-standard": "^6.2",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^9.1"
    }
}
